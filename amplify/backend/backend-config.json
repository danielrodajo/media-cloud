{
	"hosting": {
		"S3AndCloudFront": {
			"service": "S3AndCloudFront",
			"providerPlugin": "awscloudformation"
		}
	},
	"function": {
		"mediacloudauthDefineAuthChallenge": {
			"build": true,
			"providerPlugin": "awscloudformation",
			"service": "Lambda"
		},
		"mediacloudauthCreateAuthChallenge": {
			"build": true,
			"providerPlugin": "awscloudformation",
			"service": "Lambda"
		}
	},
	"auth": {
		"userPoolGroups": {
			"service": "Cognito-UserPool-Groups",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "auth",
					"resourceName": "mediacloudauth",
					"attributes": [
						"UserPoolId",
						"AppClientIDWeb",
						"AppClientID",
						"IdentityPoolId"
					]
				}
			]
		},
		"mediacloudauth": {
			"service": "Cognito",
			"providerPlugin": "awscloudformation",
			"dependsOn": []
		}
	},
	"storage": {
		"mediaclouds3": {
			"service": "S3",
			"providerPlugin": "awscloudformation"
		}
	}
}